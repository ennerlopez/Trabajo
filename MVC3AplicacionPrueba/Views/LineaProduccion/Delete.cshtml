@model MVC3AplicacionPrueba.Models.LineasProduccion

@{
    ViewBag.Title = "Lineas Produccion";
}


@using (Html.BeginForm()) {
     <table class="chooseLista colorBlanco Negrita">
            <tr>
                <td class="colorAmarillo" colspan="2">Eliminar Linea Produccion</td>
            </tr>
            <tr>
                <td colspan="2">¿Esta seguro que Desea Eliminar Esto?</td>
            </tr>
         <tr>
             <td>Nombre:</td>
             <td>   @Html.DisplayFor(model => model.nombreLinea)</td>
               
         </tr>
         <tr>
             <td>Dirección:</td>
             <td>   @Html.DisplayFor(model => model.comentarioLinea)</td>
               
         </tr>
         <tr>
             <td></td>
             <td> <input type="submit" value="Eliminar" /></td>
         </tr>
        </table>
    
}
<br/>
<div style="height: 48px;" >
    
    <a class="BotonBack" href="@Url.Action("Index")">
       <img alt="" src="@Url.Content("~/Content/BotonesTabla/back.png")" /> 
    </a>

</div>

