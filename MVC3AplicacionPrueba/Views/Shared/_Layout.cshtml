@using MVC3AplicacionPrueba.Models
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    
    <!--Estilo Principal --> 
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Site.css")"/>
    <!--==================================================================================================== -->
    
    <!--Libreria Principal Jquery -->
    <script type="text/javascript" src="@Url.Content("~/Scripts/Jquery Base/jquery-1.7.2.min.js")"></script>
    <!--==================================================================================================== -->
    
    <!--Declaracion de Estilos y Scripts para Los Dos Listbox -->
    <script type="text/javascript" src="@Url.Content("~/Scripts/ListBoxDoble/dlbScriptCrossBrowser.js")"></script>
    <link type="text/css" href="@Url.Content("~/Scripts/ListBoxDoble/style.css")" media="screen" rel="stylesheet" />
    <!--==================================================================================================== -->
   
    <!--Declaracion Estilos y Scripts Menu-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/MiMenu/css/dcmegamenu.css")"/>
    <script type="text/javascript" src="@Url.Content("~/Scripts/MiMenu/js/jquery.hoverIntent.minified.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/MiMenu/js/jquery.dcmegamenu.1.3.3.js")"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/MiMenu/css/skins/blue.css  ")"/>
    <script type="text/javascript" src="@Url.Content("~/Scripts/MiMenu/Menu.js")"></script>
    <!--==================================================================================================== -->
   
    <!-- Declaracion Estilos y Script DataTable En General  ==> Selector .Tabla -->
    <link type="text/css" href="@Url.Content("~/Scripts/Tabla/Tabla.css")" media="screen" rel="stylesheet" />
    <link type="text/css" href="@Url.Content("~/Scripts/Tabla/jquery.dataTables_themeroller.css")" media="screen" rel="stylesheet" />
   
    <link type="text/css" href="@Url.Content("~/Scripts/Tabla/UI/jquery-ui-1.8.22.custom.css")" media="screen" rel="stylesheet" />
   
    <script type="text/javascript" src="@Url.Content("~/Scripts/Tabla/jquery.dataTables.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Tabla/jquery.dataTables.editable.js")"></script>
    <script src="@Url.Content("~/Scripts/Tabla/jquery.jeditable.js")" type="text/javascript"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.validate.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Tabla/Tabla.js")"></script>
    <!--==================================================================================================== -->   
    
    <!--Declaracion Estilos De Los botones para alertas y sus Script -->     
    <link type="text/css" href="@Url.Content("~/Scripts/Alertas/buttons.css")" media="screen" rel="stylesheet" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/Alertas/js/noty/jquery.noty.js")"></script>
    <!--Layout para alertas define donde se muestran --> 
    <script type="text/javascript" src="@Url.Content("~/Scripts/Alertas/js/noty/layouts/top.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Alertas/js/noty/layouts/center.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Alertas/js/noty/layouts/topLeft.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Alertas/js/noty/layouts/topRight.js")"></script>
    <!-- Tema De Alertas-->
    <script type="text/javascript" src="@Url.Content("~/Scripts/Alertas/js/noty/themes/default.js")"></script>
    <!--==================================================================================================== -->

    <!--Declaracion Scripst para cambiar apariencia de las cajas de textos y si se quiere de botones -->    
    <script type="text/javascript" src="@Url.Content("~/Scripts/UI_Jquery/jquery-ui-1.8.22.custom.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/UI_Jquery/CajasTexto.js")"></script>
    <!--==================================================================================================== -->
   
   <!--Declaracion Scripst para tooltipo Botones -->    
    <script type="text/javascript" src="@Url.Content("~/Scripts/Tooltip/js/sexy-tooltips.v1.1.jquery.min.js")"></script>
    <link type="text/css" href="@Url.Content("~/Scripts/Tooltip/js/sexy-tooltips/hulk.css")" media="screen" rel="stylesheet" />
    <script type="text/javascript">
        $(document).ready(function () {
            $('.BotonNuevo').tooltip('Nuevo.', { mode: 'tr', width: 100 });
            $('.BotonBack').tooltip('Regresar.', { mode: 'tr', width: 100 });
            
        });
    </script>
   <!--==================================================================================================== -->
   
    <script  type="text/javascript" >
        $(function () {

           $(".botonUINuevo").button({

               icons: {
                   primary: 'ui-icon ui-icon-circle-plus'
               },
               label: "Nuevo"


           });


           
           
       });
	</script>
    <!--Declaracion Scripst para Validacion formularios ========================================== -->   
    <link type="text/css" href="@Url.Content("~/Content/ValidacionFormularios/css/validationEngine.jquery.css")" media="screen" rel="stylesheet" />
  
    <script charset="utf-8" type="text/javascript" src="@Url.Content("~/Scripts/ValidacionFormularios/languages/jquery.validationEngine-es.js")"></script>
    <script charset="utf-8" type="text/javascript" src="@Url.Content("~/Scripts/ValidacionFormularios/jquery.validationEngine.js")"></script>
    
    <script type="text/javascript">
        $(document).ready(function () {
            // binds form submission and fields to the validation engine
            $(".validacionFormularioGeneral").validationEngine();/*'attach', {
                onValidationComplete: function (form, status) {
                    alert("The form status is: " + status + ", it will never submit");
                } 
            });*/
        });
    </script>
    

    <!-- ============================================================================================ -->
</head>
