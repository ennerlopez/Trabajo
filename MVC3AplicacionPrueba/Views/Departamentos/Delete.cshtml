@model MVC3AplicacionPrueba.Models.Departamento

@{
    ViewBag.Title = "Eliminar Departamento";
}


@using (Html.BeginForm())
{
     <table class="chooseLista colorBlanco Negrita">
            <tr>
                <td class="colorAmarillo" colspan="2">Eliminar Departamento</td>
            </tr>
            <tr>
                <td colspan="2">¿Esta seguro que Desea Eliminar Esto?</td>
            </tr>
         <tr>
            <td>Departamento:</td>
            <td> @Html.DisplayFor(model => model.nombreDepartamento)</td>
        </tr>
         <tr>
             <td>Comentario:</td>
             <td>  @Html.DisplayFor(model => model.comentarioDepartamento)</td>
         </tr>
          <tr>
            <td>Grupo:</td>
            <td>  @Html.DisplayFor(model => model.Grupo.nombreGrupo)</td>
        </tr>
        <tr>
            <td>Linea:</td>
            <td>  @Html.DisplayFor(model => model.LineasProduccion.nombreLinea)</td>
        </tr>
         
         
         
         <tr>
             <td></td>
             <td> <input type="submit" value="Eliminar" /></td>
         </tr>
        </table>
    
}
<br/>
<div style="height: 48px;" >
    
    <a class="BotonBack" href="@Url.Action("Index")">
       <img alt="" src="@Url.Content("~/Content/BotonesTabla/back.png")" /> 
    </a>

</div>

