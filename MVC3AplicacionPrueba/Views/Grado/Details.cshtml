@model MVC3AplicacionPrueba.Models.Grado

@{
    ViewBag.Title = "Detalle de Grado";
}
 <table class="chooseLista colorBlanco Negrita">
        <tr>
            <td class="colorAmarillo" colspan="3">Detalle Operaciones</td>
        </tr>
        <tr>
            <td>Grado:</td>
            <td> @Html.DisplayFor(model => model.descripcionGrado)</td>
        </tr>
        <tr>
            <td>Valor del Grado:</td>
            <td>  @Html.DisplayFor(model => model.valorGrado)</td>
        </tr>
       
        <tr>
            <td></td>
            <td>@Html.ActionLink("Editar", "Edit", new { id=Model.codigoGrado},new{@class="btn btn-primary NoSubrayado"})</td>
        </tr>
   
    </table>


<br/>
<div style="height: 48px;" >
    
    <a class="BotonBack" href="@Url.Action("Index")">
       <img alt="" src="@Url.Content("~/Content/BotonesTabla/back.png")" /> 
    </a>

</div>
