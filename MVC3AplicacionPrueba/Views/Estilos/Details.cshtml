@model MVC3AplicacionPrueba.Models.Estilo

@{
    ViewBag.Title = "Estilo";
}

    
    <table class="chooseLista colorBlanco Negrita">
        <tr>
            <td class="colorAmarillo" colspan="3">Detalle Estilo</td>
        </tr>
        <tr>
            <td>Nombre:</td>
            <td> @Html.DisplayFor(model => model.nombreEstilo)</td>
        </tr>
        <tr>
            <td>Comentario:</td>
            <td>  @Html.DisplayFor(model => model.comentario)</td>
        </tr>
       <tr>
            <td></td>
            <td>@Html.ActionLink("Editar", "Edit", new { id=Model.codigoEstilo },new{@class="btn btn-primary NoSubrayado"})</td>
        </tr>
    </table>
    
    

<br/>
<div style="height: 48px;" >
    
    <a class="BotonBack" href="@Url.Action("Index")">
       <img alt="" src="@Url.Content("~/Content/BotonesTabla/back.png")" /> 
    </a>

</div>